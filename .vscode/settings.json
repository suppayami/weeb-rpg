{
  "editor.formatOnSave": false,
  "prettier.enable": false,
  "volar.takeOverMode.enabled": "auto",
  "search.exclude": {
    "*.lock": true
  },
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[vue]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  }
}
